{
    "name": "@eslym/rs4r",
    "version": "1.2.0-beta.0",
    "description": "Build svelte for Node-RED with rollup",
    "type": "module",
    "main": "dist/index.mjs",
    "author": "0nepeop1e",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/eslym/rollup-svelte-4-red.git"
    },
    "scripts": {
        "format": "prettier --write .",
        "build": "rollup -c",
        "debug": "cd example && rollup -c",
        "prepare": "husky install"
    },
    "types": "./index.d.ts",
    "dependencies": {
        "@rollup/plugin-alias": "^5.0.0",
        "@rollup/plugin-commonjs": "^25.0.3",
        "@rollup/plugin-node-resolve": "^15.1.0",
        "@rollup/plugin-replace": "^5.0.2",
        "@rollup/plugin-virtual": "^3.0.1",
        "@rollup/pluginutils": "^5.0.2",
        "lodash.isequal": "^4.5.0",
        "prettier": "^3.0.1",
        "rollup": "^3.27.2",
        "rollup-plugin-import-css": "^3.3.1",
        "rollup-plugin-svelte": "^7.1.6",
        "svelte": "^4"
    },
    "devDependencies": {
        "@eslym/tinybars": "^1.0.11",
        "@rollup/plugin-typescript": "^11.1.2",
        "@types/jquery": "^3.5.16",
        "@types/lodash.isequal": "^4.5.6",
        "@types/node": "^20.4.7",
        "@types/node-red": "^1.3.1",
        "glob": "^10.3.3",
        "husky": "^8.0.0",
        "tslib": "^2.6.1",
        "typescript": "^5.1.6"
    },
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.mjs",
            "types": "./index.d.ts"
        },
        "./internal": {
            "require": "./dist/internal.cjs",
            "import": "./dist/internal.mjs",
            "types": "./index.d.ts"
        },
        "./runtime": {
            "require": "./dist/runtime.cjs",
            "import": "./dist/runtime.mjs",
            "types": "./index.d.ts"
        },
        "./tray": {
            "require": "./dist/tray.cjs",
            "import": "./dist/tray.mjs",
            "types": "./index.d.ts"
        },
        "./components": {
            "svelte": "./components/index.mjs",
            "types": "./components/index.d.ts"
        }
    },
    "files": [
        "dist/**/*.mjs",
        "dist/**/*.cjs",
        "components/**/*.mjs",
        "components/**/*.svelte",
        "components/**/*.d.ts",
        "index.d.ts"
    ]
}
