<script type='module' src='resources/{{package.name}}/{{bundleEntry}}'></script>

<script type='text/javascript'>
    window.RS4R[{{{@json package.name }}}].register({{{@json name}}});
</script>

<script type='text/html' data-template-name='{{name}}'>
    <div id="{{ package.name }}/{{name}}"></div>
</script>

{{#if docContent}}

<script type='{{docType}}' data-help-name='{{name}}'>
{{{docContent}}}
</script>

{{/if}}
